//write or update operation using array
#include<stdio.h>
int main()
{
    int a[]={1,2,3,4,5};
    int n=sizeof(a)/sizeof(a[0]);
    int index=22;
    int newval=13;
    if(index>=0 && index<n){
        a[index]=newval;
        for(int i=0;i<n;i++)
        printf("%d ",a[i]);
        printf("\n");
    }
    else
    {
        printf("Index out of range");
    }
}
