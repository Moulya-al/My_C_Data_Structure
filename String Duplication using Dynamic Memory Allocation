#include <stdio.h>
#include <stdlib.h>
char* my_strdup(const char *src) {
    int len = 0;
    while (src[len] != '\0')   
        len++;

    char *dup = (char*)malloc((len + 1) * sizeof(char));
    if (dup == NULL) {
        printf("Memory allocation failed!\n");
        return NULL;
    }

    for (int i = 0; i <= len; i++)   
        dup[i] = src[i];

    return dup;
}

int main() {
    char str[100];
    printf("Enter a string: ");
    fgets(str, 100, stdin);

    char *copy = my_strdup(str);

    printf("\nOriginal String: %s", str);
    printf("\nCopied String: %s\n", copy);

    free(copy);  
    return 0;
}
