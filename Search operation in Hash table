#include <stdio.h>
#define SIZE 10
int hashTable[SIZE];
void initHashTable() {
    for (int i = 0; i < SIZE; i++)
        hashTable[i] = -1;
}
int hashFunction(int key, int size) {
    return key % size;
}
void search(int key) {
 int index = hashFunction(key, SIZE);
 int i = 0;
 while(hashTable[(index + i) % SIZE] != -1) {
 if(hashTable[(index + i) % SIZE] == key) {
 printf("%d found at index %d\n", key, (index + i) % SIZE);
 return;
 }
 i++;
 if(i == SIZE) break;
 }
 printf("%d not found\n", key);
}
void display() {
 printf("Hash Table:\n");
 for(int i = 0; i < SIZE; i++) {
 if(hashTable[i] != -1)
 printf("Index %d -> %d\n", i, hashTable[i]);
 else
 printf("Index %d -> [empty]\n", i);
 }
}

int main() {
    initHashTable();
    hashTable[3] = 23;
    hashTable[4] = 43;
    hashTable[5] = 13;
    hashTable[6] = 27;
    search(13);
    search(99);
    search(23);
    display();
    return 0;
}
